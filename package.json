{
  "name": "pb-frontend-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build --prod",
    "postinstall": "ng build --aot --prod",
    "heroku-postbuild": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "download-schema": "apollo schema:download --endpoint=http://localhost:3000/query graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.2",
    "@angular/cli": "^12.0.2",
    "@angular/common": "^12.0.2",
    "@angular/compiler": "^12.0.2",
    "@angular/compiler-cli": "^12.0.2",
    "@angular/core": "^12.0.2",
    "@angular/forms": "^12.0.2",
    "@angular/localize": "^12.0.2",
    "@angular/platform-browser": "^12.0.2",
    "@angular/platform-browser-dynamic": "^12.0.2",
    "@angular/router": "^12.0.2",
    "@apollo/client": "^3.5.6",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "apollo-angular": "^2.6.0",
    "chart.js": "^2.9.3",
    "core-js": "^2.6.9",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "ng2-charts": "^2.4.2",
    "ngx-perfect-scrollbar": "6.1.0",
    "nvm": "0.0.4",
    "pace-js": "^1.0.2",
    "path": "^0.12.7",
    "rxjs": "^6.6.0",
    "rxjs-compat": "^6.6.0",
    "sass": "^1.29.0",
    "tslib": "^2.0.0",
    "typescript": "4.2.4",
    "uglify-es": "3.2.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular/language-service": "^12.0.2",
    "@types/graphql": "^14.5.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "apollo": "^2.33.9",
    "codelyzer": "^6.0.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0"
  },
  "engine": {
    "node": "14.15.5",
    "npm": "6.14.11"
  },
  "resolutions": {
    "graphql": "14.5.0",
    "**/graphql": "^14.5.0"
  }
